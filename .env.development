# Development environment configuration
FLASK_ENV=development
FLASK_DEBUG=true
FLASK_HOST=127.0.0.1
FLASK_PORT=5000
FLASK_SECRET_KEY=dev-secret-key-change-in-production

# Data directory
DATA_DIR=./data

# Redis configuration
REDIS_URL=redis://localhost:6379/0
CACHE_ENABLED=true
CACHE_TTL_DAYS=1

# Auto import settings
# DEPRECATED: File watcher is disabled in favor of Daily Import Scheduler (2:05pm PT)
# Set to true only if you need continuous file polling (not recommended)
AUTO_IMPORT_ENABLED=false
AUTO_IMPORT_INTERVAL=30

# Logging
LOG_LEVEL=DEBUG

